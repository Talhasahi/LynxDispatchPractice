<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".saveVendorTripActivity"
    >

    <TextView
        android:id="@+id/vendortriptextView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Create Vendor Trip"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/backButton_go_create_vendortrip_dispatcher"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/ic_baseline_arrow_back_24"
        android:elevation="10dp"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vendortriptextView19">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="20dp">


            <Button
                android:id="@+id/button__vendortrip_dispatcher"
                android:layout_width="320dp"
                android:layout_height="45dp"
                android:layout_marginTop="50dp"
                android:background="@drawable/loginbutton"
                android:elevation="10dp"
                android:text="Create Vendor Trip"
                android:textColor="#FFFFFF"
                app:layout_constraintEnd_toEndOf="@+id/textInputLayout12"
                app:layout_constraintStart_toStartOf="@+id/textInputLayout12"
                app:layout_constraintTop_toBottomOf="@+id/textInputLayout12" />


            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/spinner_vehicle_vendortrip_Dispatcher2"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
                android:layout_width="320dp"
                android:layout_height="50dp"
                android:layout_marginTop="8dp"
                android:hint="Vehicle"
                app:layout_constraintEnd_toEndOf="@+id/textInputLayout17"
                app:layout_constraintStart_toStartOf="@+id/textInputLayout17"
                app:layout_constraintTop_toBottomOf="@+id/textInputLayout17">

                <AutoCompleteTextView
                    android:id="@+id/spinner_vehicle_vendortrip_Dispatcher"
                    android:layout_width="320dp"
                    android:layout_height="45dp"
                    android:filterTouchesWhenObscured="false"
                    android:inputType="none" />

            </com.google.android.material.textfield.TextInputLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/spinner_passengers_Dispatcher2"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
                android:layout_width="320dp"
                android:layout_height="50dp"
                android:layout_marginTop="8dp"
                android:hint="Passenger"
                app:layout_constraintEnd_toEndOf="@+id/spinner_vehicle_vendortrip_Dispatcher2"
                app:layout_constraintStart_toStartOf="@+id/spinner_vehicle_vendortrip_Dispatcher2"
                app:layout_constraintTop_toBottomOf="@+id/spinner_vehicle_vendortrip_Dispatcher2">

                <AutoCompleteTextView
                    android:id="@+id/spinner_passengers_vendortrip_Dispatcher"
                    android:layout_width="320dp"
                    android:layout_height="45dp"
                    android:filterTouchesWhenObscured="false"
                    android:inputType="none" />

            </com.google.android.material.textfield.TextInputLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                android:layout_width="320dp"
                android:layout_height="50dp"
                android:hint="Name"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/name__vendortrip_dispatcher"
                    android:layout_width="320dp"
                    android:layout_height="45dp"
                    android:inputType="textPersonName"
                    android:maxLines="1"
                    android:padding="10dp" />
            </com.google.android.material.textfield.TextInputLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout133"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                android:layout_width="320dp"
                android:layout_height="50dp"
                android:layout_marginTop="8dp"
                android:hint="Trip/Leg ID"
                app:layout_constraintEnd_toEndOf="@+id/spinner_passengers_Dispatcher2"
                app:layout_constraintStart_toStartOf="@+id/spinner_passengers_Dispatcher2"
                app:layout_constraintTop_toBottomOf="@+id/spinner_passengers_Dispatcher2">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/trip_id_vendortrip_dispatcher"
                    android:layout_width="320dp"
                    android:layout_height="45dp"
                    android:inputType="text"
                    android:maxLines="1"
                    android:padding="10dp" />
            </com.google.android.material.textfield.TextInputLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout5"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                android:layout_width="320dp"
                android:layout_height="50dp"
                android:layout_marginTop="8dp"
                android:hint="Contact No"
                app:layout_constraintEnd_toEndOf="@+id/textInputLayout"
                app:layout_constraintStart_toStartOf="@+id/textInputLayout"
                app:layout_constraintTop_toBottomOf="@+id/textInputLayout">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/contact_no_vendortrip_dispatcher"
                    android:layout_width="320dp"
                    android:layout_height="45dp"
                    android:inputType="number"
                    android:maxLines="1"
                    android:padding="10dp" />
            </com.google.android.material.textfield.TextInputLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout7"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                android:layout_width="320dp"
                android:layout_height="50dp"
                android:layout_marginTop="8dp"
                android:hint="Pickup Address"
                app:layout_constraintEnd_toEndOf="@+id/textInputLayout5"
                app:layout_constraintStart_toStartOf="@+id/textInputLayout5"
                app:layout_constraintTop_toBottomOf="@+id/textInputLayout5">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/pickup_address_vendortrip_dispatcher"
                    android:layout_width="320dp"
                    android:layout_height="45dp"
                    android:clickable="true"
                    android:focusableInTouchMode="false"
                    android:inputType="text"
                    android:maxLines="1"
                    android:padding="10dp" />
            </com.google.android.material.textfield.TextInputLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout9"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                android:layout_width="320dp"
                android:layout_height="50dp"
                android:layout_marginTop="8dp"
                android:hint="Dropoff Address"
                app:layout_constraintEnd_toEndOf="@+id/textInputLayout7"
                app:layout_constraintStart_toStartOf="@+id/textInputLayout7"
                app:layout_constraintTop_toBottomOf="@+id/textInputLayout7">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/dropoff_address_vendortrip_dispatcher"
                    android:layout_width="320dp"
                    android:layout_height="45dp"
                    android:clickable="true"
                    android:focusableInTouchMode="false"
                    android:inputType="text"
                    android:maxLines="1"
                    android:padding="10dp" />
            </com.google.android.material.textfield.TextInputLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout16"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                android:layout_width="320dp"
                android:layout_height="50dp"
                android:layout_marginTop="8dp"
                android:hint="Mileage"
                app:layout_constraintEnd_toEndOf="@+id/textInputLayout9"
                app:layout_constraintStart_toStartOf="@+id/textInputLayout9"
                app:layout_constraintTop_toBottomOf="@+id/textInputLayout9">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/Mileage_vendortrip_dispatcher"
                    android:layout_width="320dp"
                    android:layout_height="45dp"
                    android:inputType="number"
                    android:maxLines="1"
                    android:padding="10dp" />
            </com.google.android.material.textfield.TextInputLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout17"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                android:layout_width="320dp"
                android:layout_height="50dp"
                android:layout_marginTop="8dp"
                android:hint="Customer Rate"
                app:layout_constraintEnd_toEndOf="@+id/textInputLayout16"
                app:layout_constraintStart_toStartOf="@+id/textInputLayout16"
                app:layout_constraintTop_toBottomOf="@+id/textInputLayout16">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/CustomerRate_vendortrip_dispatcher"
                    android:layout_width="320dp"
                    android:layout_height="45dp"
                    android:inputType="number"
                    android:maxLines="1"
                    android:padding="10dp" />
            </com.google.android.material.textfield.TextInputLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout10"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                android:layout_width="320dp"
                android:layout_height="50dp"
                android:layout_marginTop="8dp"
                android:hint="Pickup Time"
                app:layout_constraintEnd_toEndOf="@+id/textInputLayout133"
                app:layout_constraintStart_toStartOf="@+id/textInputLayout133"
                app:layout_constraintTop_toBottomOf="@+id/textInputLayout133">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/pickup_time_vendortrip_dispatcher"
                    android:layout_width="320dp"
                    android:layout_height="45dp"
                    android:clickable="true"
                    android:focusableInTouchMode="false"
                    android:inputType="none"
                    android:maxLines="1"
                    android:padding="10dp" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout11"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                android:layout_width="320dp"
                android:layout_height="50dp"
                android:layout_marginTop="8dp"
                android:hint="Apt. Time (Optional)"
                app:layout_constraintEnd_toEndOf="@+id/textInputLayout10"
                app:layout_constraintStart_toStartOf="@+id/textInputLayout10"
                app:layout_constraintTop_toBottomOf="@+id/textInputLayout10">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/apt_time_vendortrip_dispatcher"
                    android:layout_width="320dp"
                    android:layout_height="45dp"
                    android:clickable="true"
                    android:focusableInTouchMode="false"
                    android:inputType="none"
                    android:maxLines="1"
                    android:padding="10dp" />
            </com.google.android.material.textfield.TextInputLayout>


            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayout12"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                android:layout_width="320dp"
                android:layout_height="50dp"
                android:layout_marginTop="8dp"
                android:hint="Date (Pickup Date)"
                app:layout_constraintEnd_toEndOf="@+id/textInputLayout11"
                app:layout_constraintStart_toStartOf="@+id/textInputLayout11"
                app:layout_constraintTop_toBottomOf="@+id/textInputLayout11">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/date_vendortrip_dispatcher"
                    android:layout_width="320dp"
                    android:layout_height="45dp"
                    android:clickable="true"
                    android:focusableInTouchMode="false"
                    android:inputType="none"
                    android:maxLines="1"
                    android:padding="10dp" />
            </com.google.android.material.textfield.TextInputLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>